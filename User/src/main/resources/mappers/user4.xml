<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user4">
	<insert id="insertUser4">
		INSERT INTO `user4` (`name`, `gender`, `age`, `addr`) VALUES(#{name}, #{gender}, #{age}, #{addr});
	</insert>
	
	<select id="selectUser4" resultType="kr.co.user.vo.User4VO">
		SELECT * FROM `user4` WHERE `seq`= #{seq};
	</select>
	
	<select id="selectUser4s" resultType="kr.co.user.vo.User4VO">
		SELECT * FROM `user4`;
	</select>
	
	<update id="updateUser4">
		UPDATE `user4` SET
			`name`=#{name},
			`gender`=#{gender},
			`age`=#{age},
			`addr`=#{addr}
		WHERE
			`seq`=#{seq};
	</update>
	
	<delete id="deleteUser4">
		DELETE FROM `user4` WHERE `seq`=#{seq};
	</delete>

	
</mapper>
